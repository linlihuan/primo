public static function inserisciObj($p) {
        $conn = Database::getConnectionPDO();
        $query = "INSERT INTO users (
                   codice 2,
                    username,
                    password,
                    cognome,
                    nome
                    )
                    VALUES (" .
                $p->getID() . "," .
                $p->getUsername() . "," .
                $p->getpassword() . "," .
                $p->getcognome() . "," . 
                $p->getnome() . "," . 
                $p->getprofilo() . ")";

        return $conn->query($query);
    }
