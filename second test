public static function modificaObj($p) {
        $conn = Database::getConnectionPDO();

        $id = $p->getID();
        $username = $p->getUsername();
        $password = $p->getpassword();
        $nome = $p->getnome();
        $cognome = $p->getcognome();
        $profilo = $p->getprofilo();

        $query = "UPDATE users SET 
                    username = $username,
                    password = $password,
                    nome = $nome,
                    cognome = $cognome,
                    profilo = $profilo
                    WHERE id = $id";
        return $conn->query($query);
    }
